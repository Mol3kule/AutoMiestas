generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model makes {
  id          Int           @id @default(autoincrement())
  make        String        @db.VarChar(40)
  make_models make_models[]
}

model make_models {
  id       Int    @id @default(autoincrement())
  make_id  Int?
  label    String @db.VarChar(60)
  makes    makes? @relation(fields: [make_id], references: [id])
  year     Int
  category String @db.VarChar(40)
}

model users {
  id            Int      @id @default(autoincrement())
  firstname     String   @db.VarChar(40)
  lastname      String   @db.VarChar(40)
  date_of_birth DateTime
  posts         posts[]
}

model posts {
  id             Int    @id @default(autoincrement())
  authorId       Int?
  authorRelation users? @relation(fields: [authorId], references: [id])
  information    Json
  images         Json
  periods        Json
  boosts         Json
}
